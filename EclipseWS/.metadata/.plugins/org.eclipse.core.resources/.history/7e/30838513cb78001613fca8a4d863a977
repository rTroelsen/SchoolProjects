package classes;

public class NameTest {
    
    public static void main(String[] args) {
        String first = Name.getUserName("Magrethe", "Mosbæk", "Dybdahl");
        System.out.println(first);
        
        String second = Name.getInitials("Magrethe", "Mosbæk", "Dybdahl");
        System.out.println(second)
    }
    
}
