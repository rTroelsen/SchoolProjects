package Lecture1;

public class P14 {

    public static void main(String[] args) {
        double money = 1000;
        money = money * 1.05;
        System.out.println("Account Balance: " + money);
        money = money * 1.05;
        System.out.println("Account Balance: " + money);
        money = money * 1.05;
        System.out.println("Account Balance: " + money);
    }

}
