package model;

public class PersonApp {
    
    public static void main(String[] args) {
        Person p = new Person("Bent", "Janus", "Christensen", 01, 07, 1993);
        p.printPerson();
        System.out.println("Initialer: " + p.getInit());
        System.out.println("Username: " + p.getUserName());
        
        System.out.println("Age: " + p.age(12, 9, 2014));
        System.out.println(p.age(19, 9, 2016));
        // int year = 2014;
        // System.out.println("Er " + year + " skud√•r: " + p.leapYear(year));
        
    }
}
