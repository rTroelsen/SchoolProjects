package PRO1;

public class P28 {

    public static void main(String[] args) {
        int distance = 8;
        int eficiency = 16;
        int price = 20;
        int gas = 4;
        double mile = 0.05;

        double result = ((distance / eficiency) * gas) + (distance * mile);
        System.out.println(result);

    }

}
