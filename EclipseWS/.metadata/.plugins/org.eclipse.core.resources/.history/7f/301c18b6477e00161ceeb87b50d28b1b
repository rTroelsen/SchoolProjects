package model;

public class PersonApp {

    public static void main(String[] args) {
        Person p = new Person("Rasmus", "Dabozz", "Troelsen", 1, 7, 1993);
        p.printPerson();
        System.out.println("Initialer: " + p.getInit());
        System.out.println("Username: " + p.getUserName());

        System.out.println("Age: " + p.age(9, 19, 2016));
        // int year = 2014;
        // System.out.println("Er " + year + " skud√•r: " + p.leapYear(year));
        System.out.println(p.leapYear(2015));

    }
}
