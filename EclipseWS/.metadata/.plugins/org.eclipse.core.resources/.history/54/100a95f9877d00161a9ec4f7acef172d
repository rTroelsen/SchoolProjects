
public class Date {
    private int date;
    private int month;

    public String getSeason(int day, int month) {
        if (month <= 0 || day <= 0) {
            return ("wut dis u talkin bout willis");
        }
        if ((month > 0 && month <= 3) && day < 21) {
            return ("dis be winter mon'");
        }
        else if ((month > 0 && month <= 3) && day >= 21) {
            return ("it be spring mon'")
        }
    }
}
