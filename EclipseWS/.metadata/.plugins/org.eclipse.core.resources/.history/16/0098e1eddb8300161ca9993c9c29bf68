package dicegames;

public class CrapsPlay {

    /**
     * The first die in the pair.
     */
    private Die die1;
    
    /**
     * The second die in the pair.
     */
    private Die die2;

    public CrapsPlay() {
        die1 = new Die(6);
        die2 = new Die(6);
    }

    public void welcomeMessage() {
        System.out.println("Welcome to CrapsPlay! Let's begin!");
    }
    
}
